<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".login_form">
    <EditText
        android:id="@+id/txtname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="91dp"
        android:layout_y="131dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="83dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="89dp"
        android:layout_y="104dp"
        android:text="Name"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="161dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="92dp"
        android:layout_y="190dp"
        android:text="Age"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="187dp" />

    <EditText
        android:id="@+id/txtAge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="89dp"
        android:layout_y="222dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="89dp"
        tools:layout_editor_absoluteY="215dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="206dp"
        android:layout_y="313dp"
        android:text="female"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="300dp" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="88dp"
        android:layout_y="313dp"
        android:text="male"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="301dp" />

    <TextView
        android:id="@+id/Sex"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_x="157dp"
        android:layout_y="280dp"
        android:text="Sex"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="272dp" />
</AbsoluteLayout>